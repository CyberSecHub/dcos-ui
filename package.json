{
  "name": "dcos-ui",
  "version": "0.1.1",
  "description": "User Interface of the DCOS",
  "main": "js/main.jsx",
  "keywords": [
    "services",
    "modules",
    "mesos",
    "mesosphere",
    "datacenter",
    "os"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mesosphere/dcos-ui"
  },
  "dependencies": {
    "MD5": "^1.2.1",
    "canvas-ui-system": "0.0.9",
    "d3": "^3.5.5",
    "flux": "^2.0.1",
    "humanize": "0.0.9",
    "jquery": "^2.1.3",
    "perfect-scrollbar": "^0.5.9",
    "react": "^0.12.2",
    "react-router": "^0.12.4",
    "react-tools": "^0.12.2",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "broccoli": "^0.13.2",
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-autoprefixer": "^2.1.0",
    "broccoli-clean-css": "^0.3.0",
    "broccoli-concat": "0.0.12",
    "broccoli-env": "0.0.1",
    "broccoli-export-tree": "^0.3.2",
    "broccoli-file-remover": "^0.3.1",
    "broccoli-less-single": "^0.2.0",
    "broccoli-lint-eslint": "^0.1.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-react": "^0.7.0",
    "broccoli-replace": "^0.2.0",
    "broccoli-static-compiler": "^0.1.4",
    "broccoli-uglify-js": "^0.1.3",
    "broccoli-webpack": "^0.1.1",
    "chalk": "^1.0.0",
    "eslint": "^0.18.0",
    "eslint-plugin-react": "^2.1.0",
    "jasmine-reporters": "^1.0.1",
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "npm-shrinkwrap": "^5.3.0"
  },
  "scripts": {
    "build": "broccoli build dist",
    "clean": "rm -rf dist tmp",
    "dist": "export BROCCOLI_ENV=\"production\" && npm run clean && npm run build",
    "serve": "export BROCCOLI_ENV=\"development\" && broccoli serve",
    "shrinkwrap": "npm-shrinkwrap --dev",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "jest/preprocessor.js",
    "setupTestFrameworkScriptFile": "jest/setupTestFramework.js",
    "modulePathIgnorePatterns": [
      "/tmp/",
      "/node_modules/",
      "/.module-cache/"
    ],
    "unmockedModulePathPatterns": [
      "MD5",
      "d3",
      "flux",
      "react",
      "react-router",
      "react-tools",
      "underscore",
      "jasmine-reporters"
    ],
    "testPathIgnorePatterns": [
      "/tmp/",
      "/node_modules/"
    ]
  }
}
