{
  "name": "dcos-ui",
  "version": "1.1.13",
  "description": "User Interface of the DCOS",
  "main": "js/index.js",
  "keywords": [
    "services",
    "modules",
    "mesos",
    "mesosphere",
    "datacenter",
    "os"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mesosphere/dcos-ui"
  },
  "engines": {
    "node": "0.8.x || 0.10.x"
  },
  "dependencies": {
    "browser-info": "0.0.3",
    "canvas-ui": "0.2.1",
    "classnames": "^2.2.3",
    "cookie": "^0.2.3",
    "d3": "^3.5.12",
    "flux": "^2.1.1",
    "less-color-lighten": "0.0.1",
    "md5": "^2.0.0",
    "mesosphere-shared-reactjs": "0.0.10",
    "moment": "^2.11.1",
    "query-string": "^3.0.0",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-gemini-scrollbar": "^2.1.0",
    "react-redux": "^4.4.0",
    "react-router": "^0.13.5",
    "react-tools": "^0.13.3",
    "react-zeroclipboard": "^3.0.0",
    "reactjs-components": "^0.14.0-beta.1",
    "reactjs-mixin": "0.0.2",
    "redux": "^3.3.1",
    "underscore": "^1.7.0",
    "zeroclipboard": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.5",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.3.19",
    "browser-sync": "^2.11.0",
    "envify": "^3.4.0",
    "eslint": "^0.24.0",
    "eslint-plugin-react": "^2.6.4",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^0.15.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.3.0",
    "gulp-jest": "^0.4.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.0",
    "gulp-replace": "^0.5.3",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "install": "^0.4.2",
    "jasmine-reporters": "^1.0.1",
    "jest-cli": "^0.8.2",
    "json-loader": "^0.5.4",
    "localStorage": "^1.0.3",
    "npm": "^3.7.1",
    "npm-shrinkwrap": "^5.4.1",
    "source-map-loader": "^0.1.3",
    "transform-loader": "^0.2.2",
    "webpack": "^1.10.1"
  },
  "scripts": {
    "clean": "rm -rf build",
    "prebuild-ee": "./scripts/validate-tests",
    "build-ee": "npm run clean && npm test && NODE_ENV='production' ./node_modules/.bin/gulp dist",
    "dist": "npm run build-ee",
    "prebuild-ce": "./scripts/validate-tests && ./scripts/prepare-ce",
    "build-ce": "npm run clean && npm test && NODE_ENV='production' ./node_modules/.bin/gulp dist",
    "serve": "NODE_ENV='development' ./node_modules/.bin/gulp serve",
    "testing": "NODE_ENV='testing' ./node_modules/.bin/gulp serve",
    "livereload": "NODE_ENV='development' ./node_modules/.bin/gulp livereload",
    "shrinkwrap": "npm-shrinkwrap --dev",
    "test": "jest",
    "blunt-test": "mv .babelrc .babelrc-tmp; jest; mv .babelrc-tmp .babelrc",
    "testdbg": "mv .babelrc .babelrc-tmp; node debug --harmony ./node_modules/jest-cli/bin/jest.js; mv .babelrc-tmp .babelrc"
  },
  "jest": {
    "testDirectoryName": "__tests__",
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "./node_modules/babel-jest",
    "setupTestFrameworkScriptFile": "jest/setupTestFramework.js",
    "setupEnvScriptFile": "jest/setupEnv.js",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "modulePathIgnorePatterns": [
      "/tmp/",
      "/node_modules/",
      "/.module-cache/"
    ],
    "unmockedModulePathPatterns": [
      "browser-info",
      "classnames",
      "d3",
      "events",
      "flux",
      "jasmine-reporters",
      "localStorage",
      "mesosphere-shared-reactjs",
      "md5",
      "react",
      "reactjs-components",
      "reactjs-mixin",
      "react-router",
      "react-tools",
      "src/js/constants",
      "src/js/utils",
      "structs",
      "underscore",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/tmp/",
      "/node_modules/"
    ]
  }
}
